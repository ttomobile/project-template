{
    "$schema": "https://json.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "format": "prettier \"resources/js/**/*.{js,jsx,ts,tsx,css}\" --check",
        "format:fix": "prettier \"resources/js/**/*.{js,jsx,ts,tsx,css}\" --write",
        "lint": "eslint \"resources/js/**/*.{js,jsx,ts,tsx}\"",
        "lint:fix": "npm run lint -- --fix",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "devDependencies": {
        "@eslint/js": "^9.13.0",
        "@tailwindcss/vite": "^4.0.0",
        "axios": "^1.11.0",
        "concurrently": "^9.0.1",
        "eslint": "^9.13.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "^5.2.1",
        "laravel-vite-plugin": "^2.0.0",
        "prettier": "^3.3.3",
        "tailwindcss": "^4.0.0",
        "vite": "^7.0.7",
        "vitest": "^2.1.4"
    }
}
